{
  "name": "angular2-seed",
  "version": "1.0.0",
  "description": "A simple starter Angular2 project",
  "scripts": {
    "build": "webpack --progress",
    "build:prod": "NODE_ENV=production webpack --progress",
    "build:aot:prod": "ngc && set NODE_ENV=production && webpack --config webpack.config.aot.js --progress",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --progress --port 3000 --content-base src",
    "start": "npm run server",
    "ngc": "ngc"
  },
  "contributors": [
    "Rob Wormald <robwormald@gmail.com>",
    "PatrickJS <github@gdi2290.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^2.2.2",
    "@angular/compiler": "^2.2.2",
    "@angular/compiler-cli": "^2.2.2",
    "@angular/core": "^2.2.2",
    "@angular/forms": "^2.2.2",
    "@angular/http": "^2.2.2",
    "@angular/platform-browser": "^2.2.2",
    "@angular/platform-browser-dynamic": "^2.2.2",
    "@angular/platform-server": "^2.2.2",
    "@angular/router": "^3.2.2",
    "@angular/upgrade": "^2.2.2",
    "angular2-in-memory-web-api": "0.0.21",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "ie-shim": "^0.1.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "~0.6.26"
  },
  "devDependencies": {
    "@ngtools/webpack": "^1.2.13",
    "@types/node": "^6.0.48",
    "angular2-router-loader": "^0.3.4",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^2.2.4",
    "css-loader": "^0.25.0",
    "html-webpack-plugin": "^2.29.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.10",
    "to-string-loader": "^1.1.4",
    "typescript": "~2.0.10",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "2.1.0-beta.11",
    "webpack-merge": "^0.15.0"
  },
  "keywords": [
    "Angular2",
    "angular2-seed",
    "official angular 2 seed",
    "official angular2 seed"
